{
  "name": "redux-service",
  "version": "0.0.7",
  "description": "[Redux Middleware] Use generator-based 'Service' to handle '(highly mutable || remote) data source && async logic'",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel index.src.js -o index.js",
    "build-dev": "cross-env babel index.src.js -o index.js -w",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThatBean/redux-service/"
  },
  "keywords": [
    "redux",
    "middleware",
    "service",
    "entry",
    "generator",
    "mutable",
    "async"
  ],
  "author": "ThatBean",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "standard": "^8.6.0"
  }
}
